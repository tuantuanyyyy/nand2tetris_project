/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in= sel, out= o1 );
    And(a = a, b = o1, out = o2);
    And(a= b, b= sel, out= o3);
    Or(a= o3, b= o2, out= out);
    //// Replace this comment with your code.
}